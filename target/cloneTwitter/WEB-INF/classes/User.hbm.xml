<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
	'_//Hibernate/Hibernate Mapping DTD 3.0//EN'
	'http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd'>
	
<hibernate-mapping>
	<class entity-name='amu.m2sir.malodumont.beans.User' name='amu.m2sir.malodumont.beans.User' table='USER' dynamic-update='false' dynamic-insert='false'>
		<cache usage="read-write"/>
		<id name="mail" column="mail" type="java.lang.String"><generator class="assigned"/></id>
		<property name='pwd' type='java.lang.String' update='true' insert='true'
				access='property' column='pwd'/>
	</class>
</hibernate-mapping>